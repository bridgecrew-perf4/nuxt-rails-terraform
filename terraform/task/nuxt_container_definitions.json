[
  {
    "name": "nuxt-container",
    "image": "667123667048.dkr.ecr.ap-northeast-1.amazonaws.com/nuxt-ecr:latest",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "nuxt",
        "awslogs-group": "/ecs/nuxt"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 80
      }
    ],
     "command": [
      "yarn", "start"
    ]
  }
]
